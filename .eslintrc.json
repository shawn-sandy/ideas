{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": [
    "plugin:vue/essential",
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "no-console": "warn",
    "no-alert": "error",
    "no-debugger": "error"

  }
}
